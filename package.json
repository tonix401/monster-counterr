{
  "name": "monster-counterr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 14120 --strictPort",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node ./start.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "docker:build": "docker build -t monster-counterr .",
    "docker:run": "docker run -d -p 8080:80 --name monster-counterr monster-counterr",
    "docker:stop": "docker stop monster-counterr && docker rm monster-counterr",
    "docker:dev": "npm run docker:build && npm run docker:run"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.46.4",
    "vite": "7.2.4"
  }
}
